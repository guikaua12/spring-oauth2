spring.security.oauth2.client.registration.google.client-id=google-client-id
spring.security.oauth2.client.registration.google.client-secret=google-client-secret
logging.level.org.springframework.security=trace
spring.security.jwt.secret=${JWT_SECRET:some_secure_jwt_secret}
spring.security.jwt.expire_seconds=${JWT_EXPIRE_SECONDS:3600}
spring.security.cors.origin=${CORS_ORIGIN:http://localhost:3000}
spring.datasource.url=jdbc:mysql://localhost/spring_client_server
spring.datasource.username=root
spring.datasource.password=
spring.jpa.properties.hibernate.dialect=org.hibernate.dialect.MySQL8Dialect
spring.jpa.hibernate.ddl-auto=update